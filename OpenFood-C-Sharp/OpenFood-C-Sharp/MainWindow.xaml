<Window x:Class="OpenFood_C_Sharp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OpenFood_C_Sharp"
        mc:Ignorable="d"
        Background="LightGray"
        Title="Star Wars Api" Height="632" Width="968.2">
    <Grid HorizontalAlignment="Left" Height="601" VerticalAlignment="Top" Width="448" Margin="0,0,-6.4,0">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="15*"/>
            <ColumnDefinition Width="20*"/>
        </Grid.ColumnDefinitions>
        <Button x:Name="peopleButton" Content="People" HorizontalAlignment="Left" Height="40" Margin="10,120,0,0" VerticalAlignment="Top" Width="169"/>
        <Button x:Name="planetsButton" Content="Planets" HorizontalAlignment="Left" Height="40" Margin="10,165,0,0" VerticalAlignment="Top" Width="169"/>
        <Button x:Name="filmsButton" Content="Films" HorizontalAlignment="Left" Height="40" Margin="10,210,0,0" VerticalAlignment="Top" Width="169"/>
        <Button x:Name="speciesButton" Content="Species" HorizontalAlignment="Left" Height="40" Margin="10,255,0,0" VerticalAlignment="Top" Width="169"/>
        <Button x:Name="vehiclesButton" Content="Vehicles" HorizontalAlignment="Left" Height="40" Margin="10,300,0,0" VerticalAlignment="Top" Width="169"/>
        <Button x:Name="starshipsButton" Content="Starships" HorizontalAlignment="Left" Height="40" Margin="10,345,0,0" VerticalAlignment="Top" Width="169"/>
        <ListBox x:Name="ListElements" HorizontalAlignment="Left" Height="536" Margin="10,55,0,0" VerticalAlignment="Top" Width="232" Grid.Column="1">


        </ListBox>
        <TabControl x:Name="mainTab" TabStripPlacement="Top" Grid.Column="1" Margin="254,10,-504,10">

        </TabControl>
        <TextBox x:Name ="search" Grid.Column="1" HorizontalAlignment="Left" Height="33" Margin="10,17,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="232"/>
    </Grid>
</Window>

